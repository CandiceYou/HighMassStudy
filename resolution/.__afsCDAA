dir=160225_600reference
rm ../params/individual_fit_param.txt
cp ResolutionFit_ind.C ../scripts/
cp fitFunction.c ../scripts/
mkdir ~/www/HighMass/$dir/
cp ~/www/index.php ~/www/HighMass/$dir/

root -l -n -b -q "../scripts/ResolutionFit_ind.C" > ../params/out_single.txt

cp ../single/*.png ~/www/HighMass/$dir/

bash readParam_single.sh

root -l -n -b -q "../scripts/plotVar_fitInd_4mu.C" > ../params/out_indiFit_4mu.txt
root -l -n -b -q "../scripts/plotVar_fitInd_4e.C" > ../params/out_indiFit_4e.txt
root -l -n -b -q "../scripts/plotVar_fitInd_2e2mu.C" > ../params/out_indiFit_2e2mu.txt

cp ../params/*.png ~/www/HighMass/$dir/

cp ResolutionFit_tmpl.C ResolutionFit.C

bash readParam_ind.sh

cp ResolutionFit.C ../scripts/

root -l -n -b -q "../scripts/ResolutionFit.C" > ../params/out_simFit_All.txt

bash readParam_sim.sh

root -l -n -b -q "../scripts/plotVar_fitSim_4mu.C" 
root -l -n -b -q "../scripts/plotVar_fitSim_4e.C" 
root -l -n -b -q "../scripts/plotVar_fitSim_2e2mu.C"

cp ../params/*.png ~/www/HighMass/$dir/ 
